match /sandbox/
  repository sandbox
  branch master
  prefix trunk/
end match

match /sandbox-branches/
  repository sandbox
  branch master
  prefix branches/
end match

match /sandbox-tags/
  repository sandbox
  branch master
  prefix tags/
end match
