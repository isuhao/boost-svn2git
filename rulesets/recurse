match /tags/(VERSION|Version|version)_(\d)\D(\d+)\D(\d+)/
  action recurse
end match

match /tags/release/(Boost|release|VERSION|Version|version)_(\d)\D(\d+)\D(\d+)/
  action recurse
end match
