match /trunk/libs/${LIBRARY}/
  min revision 38328
  repository ${REPOSITORY}
  branch master
end match
match /trunk/boost/${LIBRARY}/
  min revision 38328
  repository ${REPOSITORY}
  prefix include/boost/${LIBRARY}/
  branch master
end match

match /devel/libs/${LIBRARY}/
  min revision 38327
  max revision 38327
  repository ${REPOSITORY}
  branch master
end match
match /devel/boost/${LIBRARY}/
  min revision 38327
  max revision 38327
  repository ${REPOSITORY}
  prefix include/boost/${LIBRARY}/
  branch master
end match

match /trunk/boost/libs/${LIBRARY}/
  max revision 38326
  repository ${REPOSITORY}
  branch master
end match
match /trunk/boost/boost/${LIBRARY}/
  max revision 38326
  repository ${REPOSITORY}
  prefix include/boost/${LIBRARY}/
  branch master
end match

